    "def classify_title(name):\n",
    "\n",
    "\ttitle_system_message = \"You are a helpful asssitant. Answer with one word.\"\n",
    "\ttitle_user_message = \"What is the title of '{0}'? Focus on just this person's title.\".format(name) \n",
    "\n",
    "\ttitle_completion = client.chat.completions.create(\n",
    "\t\tmodel=model,\n",
    "\t\tmessages=[\n",
    "        \t{\"role\": \"system\", \"content\": title_system_message},\n",
    "\t\t\t{\"role\": \"user\", \"content\": title_user_message},\n",
    "\t\t],\n",
    "\t)\n",
    "\t\n",
    "\ttitle = f\"{title_completion.choices[0].message.content}\"\n",
    "\n",
    "\treturn title"
   ]
  },
