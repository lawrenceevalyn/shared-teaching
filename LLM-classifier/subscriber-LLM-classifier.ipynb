   "source": [
    "def classify_gender(name):\n",
    "\t\n",
    "\tgender_system_message = \"You are a helpful asssitant. Answer with one letter.\"\n",
    "\tgender_user_message = \"What is the gender of '{0}'?\".format(name) \n",
    "\t\n",
    "\tgender_completion = client.chat.completions.create(\n",
    "\t\tmodel=model,\n",
    "\t\tmessages=[\n",
    "        \t{\"role\": \"system\", \"content\": gender_system_message},\n",
    "\t\t\t{\"role\": \"user\", \"content\": gender_user_message},\n",
    "\t\t],\n",
    "\t)\n",
    "\t\n",
    "\tgender = f\"{gender_completion.choices[0].message.content}\"\n",
    "\n",
    "\treturn gender"
   ]
  },
